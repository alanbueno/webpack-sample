{
  "name": "webpack-sample",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "webpack-dev-server --progress --open --config webpack/config.dev.js",
    "build": "yarn clear:build && webpack --config webpack/config.prod.js",
    "build:ts": "tsc",
    "test": "yarn build && yarn test:unit && yarn test:integration",
    "test:integration": "PORT=8000 server-test :8000 cypress:run",
    "clear:build": "rm -rf ./build",
    "clear:all": "yarn clear:build && yarn clear:cypress:videos",
    "lint": "eslint '**/*.ts'",
    "lint:fix:dry": "eslint '**/*.ts' --fix-dry-run",
    "cypress:open": "yarn start & cypress open",
    "cypress:run": "cypress run",
    "clear:cypress:videos": "rm -rf ./cypress/videos",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "keywords": [
    "react",
    "saga",
    "webpack-sample",
    "typescript"
  ],
  "dependencies": {
    "@sentry/browser": "^5.15.3",
    "apisauce": "^1.1.1",
    "husky": "^4.2.3",
    "md5": "^2.2.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-loadable": "^5.5.0",
    "react-pdf-js": "^5.1.0",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.1.2",
    "react-toastify": "^5.5.0",
    "redux-persist": "^6.0.0",
    "redux-saga": "^1.1.3",
    "start-server-and-test": "^1.10.11",
    "ts-loader": "^6.2.2",
    "typescript": "^3.8.3"
  },
  "devDependencies": {
    "@storybook/addon-actions": "6.3.2",
    "@storybook/addon-info": "5.3.17",
    "@storybook/addon-knobs": "5.3.17",
    "@storybook/addon-links": "5.3.17",
    "@storybook/addons": "5.3.17",
    "@storybook/react": "5.3.17",
    "@types/react": "^16.9.26",
    "@types/react-dom": "^16.9.5",
    "@types/react-redux": "^7.1.7",
    "@types/react-router-dom": "^5.1.3",
    "@types/storybook-addon-jsx": "^7.0.1",
    "@types/storybook__react": "^5.2.1",
    "@typescript-eslint/eslint-plugin": "^2.25.0",
    "@typescript-eslint/parser": "^2.25.0",
    "cypress": "^4.2.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-config-standard": "^14.1.1",
    "eslint-config-standard-jsx": "^8.1.0",
    "eslint-plugin-cypress": "^2.10.3",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^3.0.0",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-standard": "^4.0.1",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.0.2",
    "marked": "^0.8.2",
    "prettier": "^2.0.2",
    "source-map-loader": "^0.2.4",
    "style-loader": "^1.1.3",
    "val-loader": "^2.1.0",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-me": "^1.2.2",
    "webpack-merge": "^4.2.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}